{
  "name": "@atrilabs/webapp-builder",
  "version": "0.0.35",
  "description": "> TODO: description",
  "author": "cruxcode <swaroopshyam0@gmail.com>",
  "homepage": "https://github.com/cruxcode/atrilabs-engine#readme",
  "license": "ISC",
  "main": "lib/index.js",
  "bin": "lib/app/app.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib",
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cruxcode/atrilabs-engine.git"
  },
  "scripts": {
    "buildExe": "tsc --project app.tsconfig.json",
    "packageExe": "yarn run buildExe && pkg --no-bytecode --public-packages '*' --public --out-path ./dist .",
    "clean": "rimraf lib && rimraf dist",
    "build": "build-atri-tool",
    "start": "dev-atri-tool",
    "server": "bootstrap-atri-services",
    "buildManifests": "build-manifest-pkg --freeze",
    "generateApp": "run-atri-target --task generate --target 'Web App'",
    "buildApp": "run-atri-target --task build --target 'Web App'",
    "prepublishOnly": "yarn run build"
  },
  "bugs": {
    "url": "https://github.com/cruxcode/atrilabs-engine/issues"
  },
  "dependencies": {
    "@atrilabs/action-layer": "^0.0.35",
    "@atrilabs/app-design-forest": "^0.0.35",
    "@atrilabs/app-design-layer": "^0.0.35",
    "@atrilabs/app-generator": "^0.0.35",
    "@atrilabs/app-page-layer": "^0.0.35",
    "@atrilabs/app-template-layer": "^0.0.35",
    "@atrilabs/asset-manager-layer": "^0.0.35",
    "@atrilabs/atri-icon-layer": "^0.0.35",
    "@atrilabs/base-layer": "^0.0.35",
    "@atrilabs/canvas-breakpoint-layer": "^0.0.35",
    "@atrilabs/canvas-runtime": "^0.0.35",
    "@atrilabs/component-icon-manifest-schema": "^0.0.35",
    "@atrilabs/component-list-layer": "^0.0.35",
    "@atrilabs/component-style-layer": "^0.0.35",
    "@atrilabs/component-tree-to-app": "^0.0.35",
    "@atrilabs/custom-props-layer": "^0.0.35",
    "@atrilabs/design-system": "^0.0.35",
    "@atrilabs/forest": "^0.0.35",
    "@atrilabs/layer-types": "^0.0.35",
    "@atrilabs/load-forest-data-layer": "^0.0.35",
    "@atrilabs/manage-canvas-runtime-layer": "^0.0.35",
    "@atrilabs/overlay-hints-layer": "^0.0.35",
    "@atrilabs/publish-app-layer": "^0.0.35",
    "@atrilabs/react-component-manifest-schema": "^0.0.35",
    "@atrilabs/react-component-manifests": "^0.0.35",
    "@atrilabs/scripts": "^0.0.35",
    "@atrilabs/server-client": "^0.0.35",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "typescript": "^4.6.4",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/plugin-syntax-flow": "^7.16.7",
    "@babel/plugin-transform-react-jsx": "^7.17.3",
    "eslint": "^8.13.0",
    "eslint-config-react-app": "^7.0.1",
    "pkg": "^5.8.0",
    "prettier": "^2.6.2"
  },
  "pkg": {
    "assets": [
      "lib/**/*",
      "src/**/*",
      "node_modules/lowdb/**/*",
      "node_modules/.cache/@atrilabs/build/manifests",
      "node_modules/yarn/**/*"
    ]
  }
}
